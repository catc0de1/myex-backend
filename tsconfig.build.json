{
  "extends": "./tsconfig.json",
  "exclude": ["node_modules", "test", "dist", "**/*spec.ts"],
  "include": ["src"],
  "compilerOptions": {
    "paths": {
      "@/*": ["src/*"],
      "@users/*": ["src/users/*"],
      "@items/*": ["src/items/*"],
      "@auth/*": ["src/auth/*"],
      "@config/*": ["src/config/*"],
      "@interceptors/*": ["src/interceptors/*"],
      "@guard/*": ["src/guard/*"],
      "@session/*": ["src/session/*"],
    }
  }
}
